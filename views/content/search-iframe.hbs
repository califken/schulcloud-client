{{#extend "lib/loggedin" hideSearch="true"}}
    {{#content "page"}}
        <style>iframe{
            width: 100%;
            height: 3500px;
            outline: none;
            border: 0px;
        }</style>
        <script>
          function resizeIframe(obj) {
            obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
          }
        </script>
        <iframe src="http://localhost:8080/#/search?noframe=true" frameborder="0" scrolling="no" onload="resizeIframe(this)"></iframe>
    {{/content}}
{{/extend}}
