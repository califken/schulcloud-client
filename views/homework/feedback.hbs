<section id="feedback" class="tab-content section-homeworkdetails">
    <div class="homework">
        <b class="grade">{{{submission.gradeText}}}</b>

        <div class="comment">
            {{#if submission.gradeComment}}
                {{{submission.gradeComment}}}
            {{else}}
                <p>Kein Kommentar.</p>
            {{/if}}
        </div>
    </div>
</section>